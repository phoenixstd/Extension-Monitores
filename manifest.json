{
    "manifest_version": 3,
    "name": "Phoenix Studio - Busqueda Tippers",
    "author":"sebastian garzon",
    "description": "Esta extension que busca los tippers de chaturbate en la transmision de la modelos y envia mensajes a esos tippers",
    "version": "2.4",
    "action": {
        "default_popup": "popup.html"
    },
    "icons": {
        "128": "images/estudio.png"
    },
    "permissions": [
        "activeTab",
        "alarms",
        "tabs",
        "scripting",
        "storage",
        "notifications"
    ],
    "host_permissions": [
        "http://*/*",
        "https://*/*"
    ],
    "background": {
        "service_worker": "js/background.js"
    },
    "content_scripts": [{
        "matches": [
            "https://*.chaturbate.com/*", "https://chaturbate.com/"
        ],
        "js": [
            "js/jquery-3.4.1.min.js",
            "js/content.js"
        ],
        "run_at": "document_end"
    }]
}